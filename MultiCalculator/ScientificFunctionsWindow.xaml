<Window x:Class="MultiCalculator.ScientificFunctionsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MultiCalculator"
        xmlns:calculatorbuttons="clr-namespace:MultiCalculator.Controls"
        mc:Ignorable="d"
        Title="ScientificFunctionsWindow" Height="400" Width="600">
    <Grid>
        <calculatorbuttons:ScientificFunctions x:Name="gridA" Width="500" Height="240" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <Button Content="Clickme" Grid.Row="4" Grid.Column="4" FontFamily="Courier New" Height="70" Width="70" VerticalAlignment="Bottom" HorizontalAlignment="Right" Click="ButtonToggle_Click"/>
    </Grid>
</Window>
